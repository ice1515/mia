{
    "model_name": "GraphSage",
    "dataset": "DD",

    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 64,
        "train_size": 250,
        "test_size": 250,
        "init_lr": 3e-3,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "epoch_interval": 100,

        "num_hidden_layers": 1,
        "hidden_dim": 112,
        "readout": "mean",
        "dropout": 0.0,
        "batch_norm": false,
        "residual": false,
        "use_node_attr": true
    }
}